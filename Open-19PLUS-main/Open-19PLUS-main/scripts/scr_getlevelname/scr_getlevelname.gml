function scr_getlevelname(level)
{
    switch (level)
    {
        case "tutorial":
            return "Buried Pizza Ruins";
        
        case "entry":
            return "Archaic Entry";
        
        case "medieval":
            return "Pizzascape";
        
        case "ruin":
            return "Ancient Cheese";
        
        case "dungeon":
            return "Bloodsauce Dungeon";
        
        case "dragonslair":
            return "Dragons Lair";
    }
}
